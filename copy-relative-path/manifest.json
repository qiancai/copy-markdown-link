  {
    "manifest_version": 3,
    "name": "Copy Relative Path",
    "version": "1.0",
    "description": "A Chrome extension to copy the relative path from a specific element in TiDB documentation.",
    "permissions": ["activeTab", "clipboardWrite", "scripting"],
    "action": {
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }